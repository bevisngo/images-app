{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/server/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/read-service",
  "projects": {
    "read-service": {
      "type": "application",
      "root": "apps/read-service",
      "entryFile": "main",
      "sourceRoot": "apps/read-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/read-service/tsconfig.app.json"
      }
    },
    "write-service": {
      "type": "application",
      "root": "apps/write-service",
      "entryFile": "main",
      "sourceRoot": "apps/write-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/write-service/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "consumer": {
      "type": "application",
      "root": "apps/consumer",
      "entryFile": "main",
      "sourceRoot": "apps/consumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/consumer/tsconfig.app.json"
      }
    }
  }
}